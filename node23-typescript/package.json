{
  "name": "try-node23-typescript",
  "version": "1.0.0",
  "description": "try nodejs23 with typescript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-strip-types --experimental-test-coverage --test",
    "test:ts": "node --experimental-strip-types --experimental-test-coverage --test **/*.test.ts",
